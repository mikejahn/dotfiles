[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  lg = log -p
  po = push origin
  lol = log --graph --decorate --oneline
  lola = log --graph --decorate --oneline --all
  shas = log --pretty=format:"%H"
  sha = log --pretty=format:"%H" -1

[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
